<odoo>
    <data>
        <!-- actions opening views on models -->
        <record model="ir.actions.act_window" id="maiconkkl.list">
            <field name="name">tree_maiconkkl</field>
            <field name="res_model">wizard.import.nfe</field>
            <field name="view_type">tree</field>
            <field name="view_mode">form,tree</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">Create the first course
                </p>
            </field>
        </record>

        <!-- Top menu item -->
        <menuitem name="maiconkkl" id="maiconkkl.menu_root"/>

        <!-- menu categories -->
        <menuitem name="Menu 1" id="maiconkkl.menu_1" parent="maiconkkl.menu_root"/>

        <!-- actions -->
        <menuitem name="Listar" id="maiconkkl.menu_1_list" parent="maiconkkl.menu_1"
                  action="maiconkkl.list"/>
    </data>
</odoo>